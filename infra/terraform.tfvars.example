# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars is gitignored by default

aws_region             = "us-east-1"
project_name           = "trialstream"
environment            = "dev"
github_repo            = "YOUR_GITHUB_USERNAME/trialstream"  # e.g., "jesseglass/trialstream"
terraform_state_bucket = "YOUR_TFSTATE_BUCKET_NAME"          # e.g., "trialstream-tfstate-12345"
google_client_id     = "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com"
google_client_secret = "YOUR_GOOGLE_CLIENT_SECRET"
